{
  "name": "TakeOverParam",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "特定のパラメータを引き継ぐ",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage"
  ],

  "icons" : {
    "48" : "img/icon48.png"
  },

  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon" : "img/icon19.png",
    "default_title" : "設定",
    "default_popup" : "options/options.html"
  }
}
