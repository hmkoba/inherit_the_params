{
  "name": "TakeOverParam",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "特定のパラメータを引き継ぐ",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage"
  ],

  "icons" : {
    "48" : "img/icon48.png"
  },

  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}